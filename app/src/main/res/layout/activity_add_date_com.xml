<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/chooesDate"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="ims.informationmanagementsystem.computer.Datesheet.AddDateCom">

    <EditText
        android:id="@+id/etComeDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="31dp"
        android:ems="10"
        android:inputType="textPersonName"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="63dp"
        tools:layout_editor_absoluteY="41dp" />

    <ImageView
        android:id="@+id/imCom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/chooseDate"
        android:layout_alignStart="@+id/chooseDate"
        android:layout_below="@+id/etComeDate"
        android:layout_marginTop="110dp"
        app:srcCompat="@mipmap/ic_launcher"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="143dp"
        tools:layout_editor_absoluteY="128dp" />

    <Button
        android:id="@+id/btnDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/chooseDate"
        android:layout_alignStart="@+id/chooseDate"
        android:layout_below="@+id/chooseDate"
        android:layout_marginTop="28dp"
        android:text="Add"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="127dp"
        tools:layout_editor_absoluteY="340dp" />

    <Button
        android:id="@+id/chooseDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imCom"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="73dp"
        android:text="Chooes"
        tools:ignore="DuplicateIds,MissingConstraints"
        tools:layout_editor_absoluteX="127dp"
        tools:layout_editor_absoluteY="243dp" />
</RelativeLayout>
